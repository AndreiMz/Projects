<div class='container'>
  <%=@videos.each_slice(2) do |v1,v2| %> 
	<div class='row'>
		<div class='col-sm'>
			<%= (Yt::Video.new id:(v1.youtube_id)).embed_html.html_safe%>
		</div>
		<div class='col-sm'>
			<%= (Yt::Video.new id:(v2.youtube_id)).embed_html.html_safe%>
		</div>
	</div>
  <%end%>	
</div>